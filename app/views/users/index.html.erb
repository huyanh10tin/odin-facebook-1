<% provide(:title, 'All Users') %>



<div class="row">
  <div class="col-md-12">
    <h1>All Users</h1>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <%= will_paginate %>
    <ul class="list-group">
      <% @users.each do |user| %>
        <li class="list-group-item">
          <h5><%= link_to user.name, user %></h5>
        </li>
      <% end %>
    </ul>
    <%= will_paginate %>
  </div>
</div>
